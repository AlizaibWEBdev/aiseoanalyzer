    <script type="text/javascript" src="https://unpkg.com/showdown/dist/showdown.min.js"></script>
    <script>
        document.getElementById('fetchForm').addEventListener('submit', async function (event) {
            event.preventDefault();

            const url = document.getElementById('url').value;
            const errorDiv = document.getElementById('error');
            const loadingDiv = document.getElementById('loading');
            const resultsDiv = document.getElementById('results');

            errorDiv.textContent = '';
            loadingDiv.style.display = 'block';
            resultsDiv.style.display = 'none';

            try {
                // Fetch SEO insights
                const response = await fetch('/fetch', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ url })
                });

                if (!response.ok) {
                    throw new Error('Failed to fetch insights');
                }

                const data = await response.json();
                const insights = data.insights;
                var converter = new showdown.Converter();

                // Display loading animation
                setTimeout(() => {
                    loadingDiv.style.display = 'none';
                    resultsDiv.style.display = 'block';
                    document.getElementById('insights').innerHTML = converter.makeHtml(insights);
                }, 1000); // Delay for animation effect

            } catch (error) {
                errorDiv.textContent = error.message;
            } finally {
                loadingDiv.style.display = 'none';
            }
        });


        let timeout;
        const pupils = document.querySelectorAll('.pupil');
        const moveInterval = 3000; // Interval to update random movement in milliseconds
        const moveRange = 12; // Maximum distance for pupil movement

        function movePupilsRandomly() {
            const angle = Math.random() * 2 * Math.PI;
            pupils.forEach(pupil => {
                const eye = pupil.parentElement;
                const rect = eye.getBoundingClientRect();
                const eyeX = rect.left + rect.width / 2;
                const eyeY = rect.top + rect.height / 2;

                // Generate random angle


                // Move pupil
                pupil.style.transform = `translate(-50%, -50%) translate(${Math.cos(angle) * moveRange}px, ${Math.sin(angle) * moveRange}px)`;
            });
        }

        // Move pupils when mouse is not moving
        function startRandomMovement() {
            movePupilsRandomly(); // Initial random movement
            setInterval(movePupilsRandomly, moveInterval);
        }

        document.addEventListener('mousemove', (event) => {
            clearTimeout(timeout);
            const { clientX: mouseX, clientY: mouseY } = event;

            pupils.forEach(pupil => {
                const eye = pupil.parentElement;
                const rect = eye.getBoundingClientRect();
                const eyeX = rect.left + rect.width / 2;
                const eyeY = rect.top + rect.height / 2;
                const angle = Math.atan2(mouseY - eyeY, mouseX - eyeX);

                pupil.style.transform = `translate(-50%, -50%) translate(${Math.cos(angle) * moveRange}px, ${Math.sin(angle) * moveRange}px)`;
            });

            timeout = setTimeout(startRandomMovement, moveInterval + 100); // Start random movement after a delay
        });

        // Start random movement when page loads
        window.onload = startRandomMovement;


    </script>
